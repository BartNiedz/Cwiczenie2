@model WolneLekturyCwiczenia.Models.SQL.Table.Epoch
@{
}
<div class="container">
    <div class="row">
        <div class="col-6">
            <form action="/Epochs/AddEpoch" method="post">

                <div class="mb-3">
                    <label class="form-label">Dodaj url</label>
                    <input type="name" asp-for="Url" class="form-control" placeholder="url">
                </div>
                <div class="mb-3">
                    <label class="form-label">Dodaj href</label>
                    <input type="name" asp-for="Href" class="form-control" placeholder="href">
                </div>
                <div class="mb-3">
                    <label class="form-label">Dodaj name</label>
                    <input type="name" asp-for="Name" class="form-control" placeholder="name">
                </div>
                <div class="mb-3">
                    <label class="form-label">Dodaj slug</label>
                    <input type="name" asp-for="Slug" class="form-control" placeholder="slug">
                </div>
                <button type="submit" class="btn btn-success">Dodaj</button>

            </form>
        </div>
    </div>
</div>